                </Setter.Value>
            </Setter>
        </Style>
    </Page.Resources>

    <Grid
        Width="1080"
        Height="1920"
        Background="White">
        <Border
            Width="766"
            Margin="0,409,0,1380"
            HorizontalAlignment="Center"
            Background="#66DEDEDE">
            <TextBox
                x:Name="nameField"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Background="#00000000"
                FocusVisualPrimaryBrush="White"
                FontSize="26"
                Foreground="Black"
                Text="NAME"
                TextAlignment="Center"
                TextWrapping="Wrap" />
        </Border>
        <Border
            Width="758"
            Margin="0,619,0,1170"
            HorizontalAlignment="Center"
            Background="#66DEDEDE">
            <TextBox
                x:Name="emailField"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Background="#00000000"
                FocusVisualPrimaryBrush="White"
                FontSize="26"
                Foreground="Black"
                Text="EMAIL"
                TextAlignment="Center"
                TextWrapping="Wrap" />
        </Border>
        <Border
            Width="765"
            Margin="0,829,0,960"
            HorizontalAlignment="Center"
            Background="#66DEDEDE">
            <TextBox
                x:Name="contactField"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Background="#00000000"
                FocusVisualPrimaryBrush="White"
                FontSize="26"
                Foreground="Black"
                Text="HP NUMBER"
                TextAlignment="Center"
                TextWrapping="Wrap" />
        </Border>
        <TextBlock
            x:Name="consentText"
            Width="643"
            Height="146"
            Margin="272,1055,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            FocusVisualPrimaryBrush="White"
            FocusVisualSecondaryBrush="#99FFFFFF"
            FontSize="18"
            Foreground="#FF636363"
            SelectionHighlightColor="{x:Null}"
            Text="Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book."
            TextAlignment="Justify"
            TextWrapping="Wrap" />
        <CheckBox
            Width="36"
            Height="35"
            Margin="164,1068,0,0"
            VerticalAlignment="Top"
            Background="#FF651693"
            BorderBrush="{x:Null}"
            Content=""
            FocusVisualPrimaryBrush="Black"
            FocusVisualSecondaryBrush="Black"
            Foreground="Black"
            Style="{StaticResource CheckBox}" />
        <Button
            x:Name="submitButton"
            Width="423"
            Height="125"
            Margin="0,1250,0,0"
            HorizontalAlignment="Center"
            VerticalAlignment="Top"
            Background="#FF651693"
            Content="SUBMIT"
            CornerRadius="30"
            FontSize="56"
            FontWeight="Bold" />
    </Grid>

</Page>
